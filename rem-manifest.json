{
  "google-calendar": {
    "2": {
      "base": "https://www.google.com/calendar/feeds",
      "params": {
        "v": "2",
        "alt": "jsonc"
      },

      "auth": ["oauth-callback", "oauth-verified"],
      "oauth": {
        "requestEndpoint": "https://www.google.com/accounts/OAuthGetRequestToken",
        "accessEndpoint": "https://www.google.com/accounts/OAuthGetAccessToken",
        "authorizeEndpoint": "https://www.google.com/accounts/OAuthAuthorizeToken",
        "version": "1.0A",
        "props": {
          "scope": "http://www.google.com/calendar/feeds"
        }
      }
    }
  },

  "youtube": {
    "2": {
      "base": "https://gdata.youtube.com/feeds/api",
      "params": {
        "v": "2",
        "alt": "jsonc"
      },
      "keyAsParam": "key",

      "auth": ["oauth-callback"],
      "oauth": {
        "requestEndpoint": "https://www.google.com/accounts/OAuthGetRequestToken",
        "accessEndpoint": "https://www.google.com/accounts/OAuthGetAccessToken",
        "authorizeEndpoint": "http://www.youtube.com/oauth_authorize_token",
        "emptyCallback": "oob",
        "version": "1.0",
        "props": {
          "scope": "http://gdata.youtube.com"
        }
      }
    }
  },

  "dropbox": {
    "1": {
      "base": [
        ["^/(files(_put)?|thumbnails)/.*", "https://api-content.dropbox.com/1"],
        "https://api.dropbox.com/1"
      ],

      "auth": ["oauth-unverified", "oauth-callback"],
      "oauth": {
        "requestEndpoint": "https://api.dropbox.com/1/oauth/request_token",
        "accessEndpoint": "https://api.dropbox.com/1/oauth/access_token",
        "authorizeEndpoint": "https://www.dropbox.com/1/oauth/authorize",
        "version": "1.0"
      }
    }
  },

  "twitter": {
    "1": {
      "name": "Twitter",
      "docs": "https://dev.twitter.com/docs",

      "base": [["^/search", "https://search.twitter.com"], "https://api.twitter.com/1"],
      "suffix": ".json",
      "postType": "form",

      "auth": ["oauth-verified", "oauth-callback"],
      "oauth": {
        "requestEndpoint": "https://api.twitter.com/oauth/request_token",
        "accessEndpoint": "https://api.twitter.com/oauth/access_token",
        "authorizeEndpoint": "https://api.twitter.com/oauth/authorize",
        "emptyCallback": "oob",
        "version": "1.0"
      }
    }
  },

  "duckduckgo": {
    "1": {
      "base": "https://api.duckduckgo.com",
      "params": {
        "format": "json",
        "no_redirect": "1"
      }
    }
  },

  "github": {
    "1": {
      "base": "https://api.github.com"
    }
  },

  "tumblr": {
    "2": {
      "base": "http://api.tumblr.com/v2",
      "keyAsParam": "api_key"
    }
  },

  "facebook": {
    "1": {
      "base": "https://graph.facebook.com",
      "postType": "form",

      "auth": ["oauth-verified", "oauth-callback"],
      "oauth": {
        "version": "2.0",
        "base": "https://graph.facebook.com",
        "props": {
          "scope": "email,read_stream"
        }
      }
    }
  }
}